<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!-- 客户资料映射 -->
<hibernate-mapping package="cn.itcast.pojo">

	<class name="Customer" table="t_Customer">
		<id name="id" type="java.lang.Long">
			<generator class="native"></generator>
		</id>
		<property name="name" length="30"></property>
		<property name="sex"></property>
		<property name="birth" type="date"></property>
		<property name="tel" length="15"></property>
		<property name="email" length="50"></property>
		<property name="address" length="200"></property>
		<property name="status"></property>
		<property name="tracecount"></property>
		
	</class>

</hibernate-mapping>



